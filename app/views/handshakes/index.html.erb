<% unless @handshake.nil? %>
  <% @handshake.each do |hand|%>
    <%= hand.name%></br>
    <%= hand.description%></br>
    <% unless hand.videos.empty?%>
      <% hand.videos.each do |video|%>
        video:</br>
        <iframe width="560" height="315" src="//www.youtube.com/embed/<%= video.url%>" frameborder="0" allowfullscreen></iframe></br>
      <% end %>
    <% end%>
  <% end%>
<% end %>
